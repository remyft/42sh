echo "----------------------------------------------------------------------------------------"
echo "                                       TESTING SET"
echo "----------------------------------------------------------------------------------------"

EX_FILE=../../21sh
CMD = SET

echo "\n\n"
echo "$CMD IN 21SH"
$EX_FILE set_no_arg 2>&1 > out1
echo "$CMD IN BASH"
bash set_no_arg 2>&1 > out2
echo "DIFF"
diff out1 out2
rm out1 out2

echo "\n\n"
echo "$CMD IN 21SH"
$EX_FILE set_invalid_arg 2>&1 > out1
echo "$CMD IN BASH"
bash set_no_arg 2>&1 > out2
echo "DIFF"
diff out1 out2

rm out1 out2
