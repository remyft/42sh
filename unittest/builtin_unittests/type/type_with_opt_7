type -afPt ls
echo "ret: $?" 

type -afPt echo
echo "ret: $?" 

type -afPt cat 
echo "ret: $?"

type -afPt kill
echo "ret: $?"

type -afPt kdsj
echo "ret: $?"

type -afPt cd
echo "ret: $?"

type -afPt alias
echo "ret: $?"

type -afPt set
echo "ret: $?"

type -afPt unset
echo "ret: $?"

type -afPt env
echo "ret: $?"

type -afPt export
echo "ret: $?"

type -afPt !
echo "ret: $?"

type -afPt ]
echo "ret: $?"

type -afPt [
echo "ret: $?"

type -afPt {
echo "ret: $?"

type -afPt }
echo "ret: $?"
