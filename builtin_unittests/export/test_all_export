echo "----------------------------------------------------------------------------------------"
echo "                              TESTING EXPORT NO ARGUMENTS"
echo "----------------------------------------------------------------------------------------"

EX_FILE=../../21sh

echo "\n\n"
echo "EXPORT IN 21SH"
$EX_FILE export_no_arg 2>&1 > out1
echo "EXPORT IN BASH"
bash export_no_arg 2>&1 > out2
echo "DIFF"
diff out1 out2

rm out1 out2

echo "----------------------------------------------------------------------------------------"
echo "                              TESTING EXPORT WITH OPTION"
echo "----------------------------------------------------------------------------------------"

echo "\n\n"
echo ">>>>> export_with_opt_0"
echo "EXPORT IN 21SH"
$EX_FILE export_with_opt_0 2>&1 > out1
echo "EXPORT IN BASH"
