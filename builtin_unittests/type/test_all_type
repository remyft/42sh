echo "----------------------------------------------------------------------------------------"
echo "                              TESTING TYPE NO ARGUMENTS"
echo "----------------------------------------------------------------------------------------"

EX_FILE=../../21sh
CMD = TYPE

echo "\n\n"
echo "$CMD IN 21SH"
$EX_FILE type_no_arg 2>&1 > out1
echo "$CMD IN BASH"
bash type_no_arg 2>&1 > out2
echo "DIFF"
diff out1 out2

rm out1 out2

echo "----------------------------------------------------------------------------------------"
echo "                              TESTING $CMD WITH OPTION"
echo "----------------------------------------------------------------------------------------"

