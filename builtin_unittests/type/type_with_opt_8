type -fpPt ls
echo "ret: $?" 

type -fpPt echo
echo "ret: $?" 

type -fpPt cat 
echo "ret: $?"

type -fpPt kill
echo "ret: $?"

type -fpPt kdsj
echo "ret: $?"

type -fpPt cd
echo "ret: $?"

type -fpPt alias
echo "ret: $?"

type -fpPt set
echo "ret: $?"

type -fpPt unset
echo "ret: $?"

type -fpPt env
echo "ret: $?"

type -fpPt export
echo "ret: $?"

type -fpPt !
echo "ret: $?"

type -fpPt ]
echo "ret: $?"

type -fpPt [
echo "ret: $?"

type -fpPt {
echo "ret: $?"

type -fpPt }
echo "ret: $?"
